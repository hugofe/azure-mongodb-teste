{
    "apiVersion":"v1",
    "kind": "Pod",
    "metadata": {
        "name": "mongo",
        "labels": {
            "app": "mongo",
            "version": "v1"
        }
    },
    "spec": {
        "containers": [
            {
                "name": "mongo",
                "image": "mongo:3.5",
                "ports": [{
                    "containerPort": 27017
                }]
            }
        ]
    }
}